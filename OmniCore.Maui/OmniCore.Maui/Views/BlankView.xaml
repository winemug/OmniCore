             xmlns:viewModels="clr-namespace:OmniCore.Maui.ViewModels"
             x:Class="OmniCore.Maui.Views.BlankView"
             x:DataType="viewModels:BlankViewModel">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="{Binding MyBeautifulText}"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>