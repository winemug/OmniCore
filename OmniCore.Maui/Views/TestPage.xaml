<?xml version="1.0" encoding="utf-8"?>

<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="OmniCore.Maui.Views.TestPage">
    <ContentPage.Content>
        <VerticalStackLayout>
            <Label>This is a test page</Label>
            <Button Text="Check Permissions" Command="{Binding CheckPermissionsCommand}"/>
            <Entry Text="{Binding Email}"/>
            <Entry Text="{Binding Password}"/>
            <Button Text="Go" Command="{Binding ButtonCommand}"/>
        </VerticalStackLayout>
    </ContentPage.Content>
</ContentPage>