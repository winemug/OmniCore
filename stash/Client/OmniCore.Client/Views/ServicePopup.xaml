<?xml version="1.0" encoding="utf-8" ?>
<pages:PopupPage xmlns="http://xamarin.com/schemas/2014/forms"
                 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                 xmlns:d="http://xamarin.com/schemas/2014/forms/design"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:pages="clr-namespace:Rg.Plugins.Popup.Pages;assembly=Rg.Plugins.Popup"
                 mc:Ignorable="d"
                 x:Class="OmniCore.Client.Views.ServicePopup">
    <Frame BackgroundColor="#DEBFC7" BorderColor="Black" Padding="20"
           VerticalOptions="CenterAndExpand" HorizontalOptions="CenterAndExpand">
        <StackLayout VerticalOptions="Center" HorizontalOptions="Center">
            <Label Text="OmniCore service is starting," HorizontalOptions="Center" />
            <Label Text="please wait." HorizontalOptions="Center" />
        </StackLayout>
    </Frame>
</pages:PopupPage>